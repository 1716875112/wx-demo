/* 轮播图 */
.index_swiper swiper image {
  width: 100%;
}
swiper{
  width: 750rpx;
  height: 340rpx;
}
/* 导航 */
.index_cate{
  display: flex;
}
.index_cate navigator{
  padding: 20rpx;
  flex: 1
}
.index_cate navigator image{
  width: 100%
}

/* 楼层 */
.floor_title{
  padding: 10rpx;

}

.floor_title image{
  width: 100%;

}
.floor_list{
  /* 清楚浮动 */
  overflow: hidden;

}
.floor_list navigator {
  float: left;
  width: 33.33%;
}
/* 后四张图片超链接 */
.floor_list navigator:nth-last-child(-n+4) {
  height: 27.727112068965514vw;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-last-child(4) {
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator:nth-last-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}
